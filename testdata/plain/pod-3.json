{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "labels": {
            "app": "nginx"
        },
        "name": "nginx-pod-3",
        "namespace": "default"
    },
    "spec": {
        "containers": [
            {
                "image": "nginx:latest",
                "imagePullPolicy": "Always",
                "name": "nginx-container",
                "ports": [
                    {
                        "containerPort": 80,
                        "protocol": "TCP"
                    }
                ],
                "resources": {}
            }
        ]
    }
}
